    <div class="block">
      <%= render :partial => '/factory/making_orders/pdf/adjustment', :locals => { :form_id_and_revision => 'F1-PCU753 Rv8'} %>
    </div>
    <div class="block last">
      <div class="separator"></div>
      <%= render :partial => '/factory/making_orders/pdf/archive', :locals => { :form_id_and_revision => 'F1-PCU753 Rv8'} %>
    </div>
